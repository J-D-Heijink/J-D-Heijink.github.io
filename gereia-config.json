{
    "path": {
        "people": "people",
        "pictures": "pictures",
        "songs": "songs",
        "logging": "log"
    },
    "extension": {
        "picture": ["jpg", "jpeg", "png"],
        "song": ["mp3"]
    },
    "recognition": {
        "jitter": 10,
        "tolerance": 0.55
    },
    "timing": {
        "loop.delay": 0.1,
        "camera.throttle": 0.250,
        "forget.recognition": 5,
        "slide.delay": 60
    },
    "logging": {
        "dissimilarity": 0.04
    },
    "camera": {
        "flip": true,
        "height": 1080,
        "input": 0,
        "width": 1920
    },
    "web": {
        "host": "0.0.0.0",
        "port": "8080",
        "upload.limit": 16384
    }
}
